<?xml version="1.0" encoding="gb2312" standalone="no"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="tel">
	<insert id="insertTel" parameterClass="java.util.HashMap">
		insert into tel (prov,city,phone) values 
		(#prov#,#city#,#tel#)
	</insert>
	
</sqlMap>